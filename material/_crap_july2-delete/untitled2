import csv
with open('/home/me/Downloads/meeeeeeeeeeeee/awk-test.txt') as f:
    reader = csv.reader(f)
    headers = next(reader)
    # transpose the data
    # --> columns become rows and rows become columns
    data = zip(*reader)
    # create a dictionary by combining the headers with the data
    d = dict(zip(headers, data))
