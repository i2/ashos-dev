def test(next_distro):
    import csv
    input_file = csv.DictReader(open('/home/me/Downloads/meeeeeeeeeeeee/awk-test.txt', 'r'), delimiter=',', quoting=csv.QUOTE_NONE)
    for row in input_file:
        if row["DISTRO"] == next_distro:
            return row["BootOrder"]

def testme_WORKS(next_distro):
    import csv
    with open('/home/me/Downloads/meeeeeeeeeeeee/awk-test.txt', 'r') as f:
        input_file = csv.DictReader(f, delimiter=',', quoting=csv.QUOTE_NONE)
        for row in input_file:
            if row["DISTRO"] == next_distro:
                return row["BootOrder"]


