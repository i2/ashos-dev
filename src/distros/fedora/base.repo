[ashos]
name=AshOS Fedora Repository
#baseurl=http://download.fedoraproject.org/pub/fedora/linux/development/$releasever/Everything/$forcearch/os
baseurl=https://mirror.csclub.uwaterloo.ca/fedora/linux/development/$releasever/Everything/$forcearch/os
enabled=1

#name=Fedora $releasever - $basearch
