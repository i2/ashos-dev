#mirrorlist=https://admin.fedoraproject.org/mirrormanager/mirrors/Fedora/development/x86_64
#enabled=1
#metadata_expire=7d
#gpgcheck=1
#gpgkey=file:///etc/pki/rpm-gpg/RPM-GPG-KEY-fedora-$basearch

